
GET     /                           controllers.HomeController.index
GET     /:entityId                  controllers.EntityController.index

# Admin
GET     /admin/about                controllers.admin.AboutController.index
GET     /admin/config               controllers.admin.ConfigController.index
GET     /admin/healthchecks         controllers.admin.MetricsController.healthChecks
GET     /admin/jvm                  controllers.admin.JVMController.index
GET     /admin/logs                 controllers.admin.LogsController.index
GET     /admin/metrics              controllers.admin.MetricsController.metrics
GET     /admin/readme               controllers.admin.ReadmeController.index
GET     /admin/routes               controllers.admin.RoutesController.index

# Static Assets
GET     /assets/*file               controllers.Assets.at(file)
GET     /versionedAssets/*file      controllers.Assets.versioned(file)
